<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.app.devonaodevo"
    android:installLocation="preferExternal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.app.devonaodevo.AcNotificacaoVencer"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.android.app.devonaodevo.AcNotificacaoVencido"
            android:screenOrientation="portrait"/>
        <activity android:name="com.android.app.devonaodevo.AcAcaoNotificacao"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.android.app.devonaodevo.AcNotification"
            android:screenOrientation="portrait" />
        <activity android:name="com.android.app.devonaodevo.AcListaGeralDetablado"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.android.app.devonaodevo.AcPrincipal"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.app.devonaodevo.AcNovaConta"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.android.app.devonaodevo.AcPagaConta"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.android.app.devonaodevo.AcPagoConta"
            android:screenOrientation="portrait" />
        <activity android:name="com.android.app.devonaodevo.AcCadAltConta"
            android:screenOrientation="portrait"/>
        <activity android:name="com.android.app.devonaodevo.AcConfiguracoes"
            android:screenOrientation="portrait"/>
        <activity android:name="com.android.app.devonaodevo.AcLancarPagar"
            android:screenOrientation="portrait"/>

        <service
            android:name="Controle.ServicoAlarme"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <service
            android:name="Controle.ServiceNovaConta"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <service
            android:name="Controle.ServiceProximosTitulosVencer"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <service
            android:name="Controle.ServiceTituloasVencidos"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />
    </application>

</manifest>